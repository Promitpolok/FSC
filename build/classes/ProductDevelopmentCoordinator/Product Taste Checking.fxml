<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductDevelopmentCoordinator.ProductTasteCheckingController">
   <children>
      <Button layoutX="220.0" layoutY="87.0" mnemonicParsing="false" onAction="#okButtonOnMouseClick" prefHeight="36.0" prefWidth="92.0" text="OK">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Label layoutX="147.0" layoutY="21.0" prefHeight="51.0" prefWidth="342.0" text="Product Taste Checking">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ComboBox fx:id="ProductNameComboBox" layoutX="48.0" layoutY="89.0" prefWidth="150.0" promptText="Product Name" />
      <TextArea fx:id="infoTextArea" editable="false" layoutX="58.0" layoutY="168.0" prefHeight="133.0" prefWidth="484.0" text="Info:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></TextArea>
      <Button layoutX="343.0" layoutY="87.0" mnemonicParsing="false" onAction="#notOkButtonOnMouseClick" prefHeight="36.0" prefWidth="92.0" text="Not OK">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="472.0" layoutY="87.0" mnemonicParsing="false" onAction="#clearButtonOnMouseClick" prefHeight="36.0" prefWidth="92.0" text="Clear ">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="254.0" layoutY="330.0" mnemonicParsing="false" onAction="#saveButtonOnMouseClick" prefHeight="36.0" prefWidth="92.0" text="Save">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="12.0" layoutY="21.0" mnemonicParsing="false" onAction="#backButtonOnMouseClick" prefHeight="36.0" prefWidth="92.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
