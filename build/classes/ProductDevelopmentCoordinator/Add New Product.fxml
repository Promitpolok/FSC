<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="448.0" prefWidth="651.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductDevelopmentCoordinator.AddNewProductController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackbuttonOnMouseClick" prefHeight="31.0" prefWidth="78.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextArea fx:id="productInfoTextArea" editable="false" layoutX="7.0" layoutY="244.0" prefHeight="200.0" prefWidth="638.0" text="OverView Product Info:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextArea>
      <ComboBox fx:id="productNameComboBox" layoutX="16.0" layoutY="66.0" prefHeight="31.0" prefWidth="153.0" promptText="Product Name" />
      <Button layoutX="549.0" layoutY="205.0" mnemonicParsing="false" onAction="#clearbuttonOnMouseClick" prefHeight="31.0" prefWidth="88.0" text="Clear">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="enterProductNameTextField" layoutX="285.0" layoutY="66.0" promptText="Enter Product Name">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <Button layoutX="346.0" layoutY="123.0" mnemonicParsing="false" onAction="#addbuttonOnMouseClick" prefHeight="31.0" prefWidth="78.0" text="Add">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
