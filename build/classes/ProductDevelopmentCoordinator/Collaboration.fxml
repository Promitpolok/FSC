<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductDevelopmentCoordinator.CollaborationController">
   <children>
      <Button layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#BackButtonOnMouseClick" prefHeight="31.0" prefWidth="83.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <ComboBox fx:id="selectTeamMemberComboBox" layoutX="14.0" layoutY="73.0" prefHeight="31.0" prefWidth="194.0" promptText="Select Team Member" />
      <TextField fx:id="projectTitleTextField" layoutX="230.0" layoutY="73.0" promptText="Enter Your Project Title">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="projectDescriptionTextField" layoutX="14.0" layoutY="131.0" promptText="Enter Project Description">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="assignTaskTextField" layoutX="230.0" layoutY="131.0" promptText="Assign Task">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextArea fx:id="infoTextArea" layoutY="200.0" prefHeight="200.0" prefWidth="600.0" text="Info">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextArea>
      <Button layoutX="455.0" layoutY="13.0" mnemonicParsing="false" onAction="#showButtonOnMouseClick" prefHeight="31.0" prefWidth="129.0" text="Show ">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="478.0" layoutY="131.0" mnemonicParsing="false" onAction="#clearButtonOnMouseClick" prefHeight="31.0" prefWidth="83.0" text="Clear">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="457.0" layoutY="73.0" mnemonicParsing="false" onAction="#sendButtonOnMouseClick" prefHeight="31.0" prefWidth="129.0" text="Send">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
