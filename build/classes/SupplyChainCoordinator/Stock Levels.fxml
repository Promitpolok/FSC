<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="437.0" prefWidth="690.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SupplyChainCoordinator.StockLevelsController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackButtonOnMouseClick" prefHeight="31.0" prefWidth="73.0" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="231.0" layoutY="74.0" mnemonicParsing="false" onAction="#CurrentStockButtonOnMouseClick" prefHeight="31.0" prefWidth="121.0" text="Current Stock">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <ComboBox fx:id="productNameComboBox" layoutX="14.0" layoutY="74.0" prefWidth="150.0" promptText="Product Name" />
      <Button layoutX="422.0" layoutY="127.0" mnemonicParsing="false" onAction="#UpdateStockLevelOnMouseClick" prefHeight="31.0" prefWidth="128.0" text="Update Stock">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextArea fx:id="viewProductInfoTextArea" editable="false" layoutX="6.0" layoutY="234.0" prefHeight="200.0" prefWidth="676.0" text="View Product Info:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextArea>
      <Button layoutX="603.0" layoutY="188.0" mnemonicParsing="false" onAction="#clearButtonOnMouseClick" prefHeight="31.0" prefWidth="73.0" text="Clear">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="enterStockForSelectedProduct" layoutX="393.0" layoutY="74.0" promptText="Enter Stock" />
      <Button layoutX="450.0" layoutY="188.0" mnemonicParsing="false" onAction="#saveButtonOnMouseClick" prefHeight="31.0" prefWidth="73.0" text="Save">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
