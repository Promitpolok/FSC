<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0598ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SupplyChainCoordinator.SupplyChainCollaborationController">
   <children>
      <Button layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#backButtonOnMouseClick" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <ComboBox fx:id="selectTeamMemberComboBox" layoutX="14.0" layoutY="71.0" prefHeight="31.0" prefWidth="193.0" promptText="Select Team Member" />
      <TextField fx:id="enterDescriptionTextField" layoutX="14.0" layoutY="120.0" promptText="Enter Project Description">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="enterProjectTitleTextField" layoutX="240.0" layoutY="71.0" prefHeight="31.0" prefWidth="187.0" promptText="Enter Your Project Title">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="assignTaskTextField" layoutX="240.0" layoutY="120.0" prefHeight="31.0" prefWidth="187.0" promptText="Assign Task">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <Button layoutX="498.0" layoutY="21.0" mnemonicParsing="false" onAction="#showButtonOnMouseClick" prefHeight="31.0" prefWidth="93.0" text="Show">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="498.0" layoutY="71.0" mnemonicParsing="false" onAction="#sendButtonOnMouseClick" prefHeight="31.0" prefWidth="93.0" text="Send">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="517.0" layoutY="120.0" mnemonicParsing="false" onAction="#clearButtonOnMouseClick" text="Clear">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextArea fx:id="infoTextArea" layoutX="-2.0" layoutY="200.0" prefHeight="200.0" prefWidth="606.0" text="Info">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextArea>
   </children>
</AnchorPane>
