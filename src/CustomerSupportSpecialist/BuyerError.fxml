<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CustomerSupportSpecialist.BuyerErrorController">
   <children>
      <Button layoutX="100.0" layoutY="282.0" mnemonicParsing="false" onAction="#StartLiveChat" text="Start Live Chat" />
      <Label fx:id="ShowErrorMessage" layoutX="80.0" layoutY="200.0" prefHeight="54.0" prefWidth="238.0" style="-fx-background-color: white;" text="Error : eg. product out of stock / onlline payment issue" textFill="#d22525" wrapText="true" />
      <Label fx:id="ShowBuyerID" layoutX="80.0" layoutY="133.0" prefHeight="54.0" prefWidth="238.0" style="-fx-background-color: white;" text="Respected Buyer ID:" textFill="#d22525" wrapText="true" />
      <Text layoutX="248.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Error">
         <font>
            <Font size="27.0" />
         </font>
      </Text>
      <Button layoutX="248.0" layoutY="331.0" mnemonicParsing="false" onAction="#CancelOrderAndNotifyBuyerDashboard" text="Cancel order and notify their dashboard" />
      <Button layoutX="28.0" layoutY="31.0" mnemonicParsing="false" onAction="#GoBackOnClick" text="Back" />
      <TextField fx:id="SendBuyerNotificationMessage" layoutX="80.0" layoutY="331.0" />
   </children>
</AnchorPane>
